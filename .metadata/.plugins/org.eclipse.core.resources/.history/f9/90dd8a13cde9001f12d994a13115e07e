package Pages;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class LoginPage {
	static WebElement email=null;
	static WebElement password=null;
	
	private static WebElement email(WebDriver driver) {
		email=driver.findElement(By.id("_58_login"));
		return email;
	} 

	private static WebElement password(WebDriver driver) {
		password=driver.findElement(By.id("_58_login"));
		return password;
	}
}
